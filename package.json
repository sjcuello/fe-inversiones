{
    "name": "inversiones",
    "version": "1.2.5",
    "scripts": {
        "prepare": "husky install",
        "start": "ng serve --base-href /obi/inversiones",
        "start:mock": "ng serve --base-href /obi/inversiones --proxyConfig proxy.mocks.conf.json",
        "start:dev": "ng serve --base-href /obi/inversiones --proxyConfig proxy.conf.json",
        "wiremock": "cd mocks && java -jar wiremock-standalone-2.27.2.jar --port 9880",
        "build": "ng build --prod --base-href /obi/inversiones/ --deploy-url=/obi/inversiones/",
        "test": "ng test",
        "test:watch": "ng test --watch",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
        "precoverage": "npm run lint",
        "coverage": "ng test --coverage",
        "lint": "ng lint",
        "lint-report": "ng lint --format=junit --force=true > test_results/obi-frontend-lint-result.xml",
        "compodoc": "npx compodoc -p tsconfig.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.0.0",
        "@angular/cdk": "^9.1.0",
        "@angular/common": "~9.0.0",
        "@angular/compiler": "~9.0.0",
        "@angular/core": "~9.0.0",
        "@angular/forms": "~9.0.0",
        "@angular/localize": "^9.1.13",
        "@angular/material": "^9.1.0",
        "@angular/platform-browser": "~9.0.0",
        "@angular/platform-browser-dynamic": "~9.0.0",
        "@angular/router": "~9.0.0",
        "@auth0/angular-jwt": "^4.0.0",
        "@ng-bootstrap/ng-bootstrap": "^6.1.0",
        "@ng-idle/core": "^8.0.0-beta.4",
        "angular-google-tag-manager": "^1.1.1",
        "bootstrap": "^4.4.1",
        "file-saver": "^2.0.5",
        "jest-canvas-mock": "^2.3.0",
        "lottie-web": "^5.7.3",
        "moment": "^2.24.0",
        "ngx-currency": "^2.5.1",
        "ngx-lottie": "^6.4.0",
        "ngx-perfect-scrollbar": "^9.0.0",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-builders/jest": "^9.0.1",
        "@angular-devkit/build-angular": "~0.900.1",
        "@angular/cli": "~9.0.1",
        "@angular/compiler-cli": "~9.0.0",
        "@angular/language-service": "~9.0.0",
        "@types/jest": "^25.2.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "glob": "^7.1.6",
        "husky": "^5.1.3",
        "jest": "^25.5.1",
        "jest-allure": "^0.1.3",
        "jest-junit": "^10.0.0",
        "jest-preset-angular": "^8.2.0",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "~3.7.5"
    }
}